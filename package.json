{
  "name": "saif-vue-tictactoe",
  "description": "Tic tac toe web app for my interview project",
  "author": "Saif Raihan",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamsaifraihan/Tic-Tac-Toe"
  },
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
